{
	"name": "minifranske/gravatar",
	"type": "typo3-cms-extension",
	"description": "AvatarProvider for Gravatar support in TYPO3 backend",
	"license": "GPL-2.0-or-later",
	"require": {
		"typo3/cms-core": "^10"
	},
	"replace": {
		"typo3-ter/gravatar": "self.version"
	},
	"autoload": {
		"psr-4": {
			"MiniFranske\\Gravatar\\": "Classes/"
		}
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "gravatar"
		}
	}
}
